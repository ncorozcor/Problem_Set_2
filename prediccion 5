#####Boosting



set.seed(69)

p_load("bst")



boosted <- train(

  price~surface_total+bedrooms+numeros_baÃ±os+estrato+v_ref+distancia_u_top+distancia_bar+distancia_disco+distancia_w,

  data=train,

  method = "bstTree",

  trControl = fitControl1,

  tuneGrid=expand.grid(

    mstop = c(300), 

    maxdepth = c(3), 

    nu = c(0.001)) 

)



